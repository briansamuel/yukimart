# Postman API Configuration
# Copy this file to .env.postman and fill in your actual values

# Postman API Key
# Get from: https://web.postman.co/settings/me/api-keys
POSTMAN_API_KEY=your_postman_api_key_here

# Workspace ID
# Get from: https://web.postman.co/workspaces (check URL or workspace settings)
POSTMAN_WORKSPACE_ID=your_workspace_id_here

# Collection ID (if updating existing collection)
# Get from: Collection settings in Postman workspace
POSTMAN_COLLECTION_ID=your_collection_id_here

# Auto Sync Settings
POSTMAN_AUTO_SYNC=true

# Collection Name
POSTMAN_COLLECTION_NAME=YukiMart API v1 - Complete với Examples

# API Base URL
API_BASE_URL=http://yukimart.local/api/v1

# Test User Credentials
TEST_USER_EMAIL=yukimart@gmail.com
TEST_USER_PASSWORD=123456

